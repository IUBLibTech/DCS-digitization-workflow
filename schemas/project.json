{
    "$defs": {
        "Contact": {
            "description": "Contact Information",
            "properties": {
                "name": {
                    "default": "__UNSET__",
                    "description": "Contact Name",
                    "title": "Name",
                    "type": "string"
                },
                "email": {
                    "default": "__UNSET__",
                    "description": "Contact Email",
                    "format": "email",
                    "title": "Email",
                    "type": "string"
                }
            },
            "title": "Contact",
            "type": "object"
        },
        "DescriptiveMetadata": {
            "description": "Project Descriptive Metadata",
            "properties": {
                "title": {
                    "default": "Project Title",
                    "title": "Title",
                    "type": "string"
                },
                "identifiers": {
                    "allOf": [
                        {
                            "$ref": "#/$defs/Identifiers"
                        }
                    ],
                    "description": "Project identifiers"
                }
            },
            "title": "DescriptiveMetadata",
            "type": "object"
        },
        "Identifiers": {
            "description": "Project Identifiers",
            "properties": {
                "iucat_barcode": {
                    "default": "",
                    "description": "IUCAT Barcode",
                    "title": "Iucat Barcode",
                    "type": "string"
                },
                "iucat_catkey": {
                    "default": "",
                    "description": "IUCAT Catalog Key",
                    "title": "Iucat Catkey",
                    "type": "string"
                },
                "call_number": {
                    "default": "",
                    "description": "Call Number",
                    "title": "Call Number",
                    "type": "string"
                },
                "other_identifiers": {
                    "description": "Other identifiers",
                    "items": {
                        "$ref": "#/$defs/ScopedIdentifier"
                    },
                    "title": "Other Identifiers",
                    "type": "array"
                }
            },
            "title": "Identifiers",
            "type": "object"
        },
        "PSystem": {
            "properties": {
                "project_id": {
                    "default": "",
                    "description": "Project ID",
                    "title": "Project Id",
                    "type": "string"
                },
                "profile": {
                    "default": "",
                    "description": "Digitizer Profile",
                    "title": "Profile",
                    "type": "string"
                },
                "physical_object_id": {
                    "default": "",
                    "description": "Physical Object ID",
                    "title": "Physical Object Id",
                    "type": "string"
                },
                "sequence_id": {
                    "default": 0,
                    "description": "Sequence ID",
                    "title": "Sequence Id",
                    "type": "integer"
                },
                "schema_name": {
                    "const": "project@v1",
                    "default": "project@v1",
                    "enum": [
                        "project@v1"
                    ],
                    "title": "Schema Name",
                    "type": "string"
                }
            },
            "title": "PSystem",
            "type": "object"
        },
        "ProjectInfo": {
            "description": "Project Information",
            "properties": {
                "creator": {
                    "default": "",
                    "description": "Project Creator Username",
                    "title": "Creator",
                    "type": "string"
                },
                "creator_name": {
                    "default": "",
                    "description": "Project Creator Name",
                    "title": "Creator Name",
                    "type": "string"
                },
                "create_date": {
                    "default": "",
                    "description": "Project creation date",
                    "title": "Create Date",
                    "type": "string"
                },
                "completion_date": {
                    "default": "",
                    "description": "Completion date",
                    "title": "Completion Date",
                    "type": "string"
                },
                "contacts": {
                    "description": "List of contacts for this project",
                    "items": {
                        "$ref": "#/$defs/Contact"
                    },
                    "minItems": 1,
                    "title": "Contacts",
                    "type": "array"
                },
                "comments": {
                    "default": "no comment",
                    "description": "Comments about the project",
                    "title": "Comments",
                    "type": "string"
                }
            },
            "title": "ProjectInfo",
            "type": "object"
        },
        "ScopedIdentifier": {
            "description": "An identifier with a scope",
            "properties": {
                "scope": {
                    "default": "__UNSET__",
                    "description": "Identifier Scope",
                    "title": "Scope",
                    "type": "string"
                },
                "identifier": {
                    "default": "__UNSET__",
                    "description": "Identifier",
                    "title": "Identifier",
                    "type": "string"
                }
            },
            "title": "ScopedIdentifier",
            "type": "object"
        }
    },
    "description": "Digitization Project",
    "properties": {
        "system": {
            "$ref": "#/$defs/PSystem"
        },
        "project_information": {
            "$ref": "#/$defs/ProjectInfo"
        },
        "descriptive_metadata": {
            "$ref": "#/$defs/DescriptiveMetadata"
        }
    },
    "title": "Project",
    "type": "object"
}